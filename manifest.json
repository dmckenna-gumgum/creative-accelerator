{
  "name": "Creative Accelerator",
  "id": "creative-accelerator",
  "version": "3.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "featureFlags": {
    "enableSWCSupport": true
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "creative-accelerator-panel",
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/pluginIcon.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/pluginIcon.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Creative Accelerator"
      },
      "minimumSize": {
        "width": 800,
        "height": 250
      },
      "maximumSize": {
        "width": 4000,
        "height": 400
      },
      "preferredDockedSize": {
        "width": 2000,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 1200,
        "height": 300
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/pluginIcon.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/pluginIcon.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "request",
    "network": {
      "domains": [
        "https://*"
      ]
    }
  },
  "host": {
    "app": "PS",
    "minVersion": "24.4.0"
  }
}